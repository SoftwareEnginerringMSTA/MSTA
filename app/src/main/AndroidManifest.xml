<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bsit.msta">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainInterface.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.bsit.msta.MainInterface.SignUp"
            android:theme="@style/MyMaterialTheme"
            android:label="REGISTER">
        </activity>
        <activity android:name=".MainInterface.StudentProfile"
            android:theme="@style/MyMaterialTheme"></activity>
        <activity android:name=".MainInterface.TeacherProfile"
            android:theme="@style/MyMaterialTheme"></activity>

        <activity android:name=".MainInterface.StudentDetails"
            android:theme="@style/MyMaterialTheme"></activity>

    </application>








</manifest>
